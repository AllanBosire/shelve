{
  "name": "@shelve/app",
  "description": "Shelve is a project management tool for developers teams",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && npm run prisma:generate --no-engine",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "vercel:env": "vercel env pull",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev"
  },
  "prisma": {
    "schema": "server/database/schema.prisma"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.1.178",
    "@linear/sdk": "^19.2.0",
    "@nuxt/fonts": "latest",
    "@nuxt/image": "latest",
    "@nuxt/ui": "^2.15.0",
    "@prisma/client": "^5.11.0",
    "@prisma/extension-accelerate": "^1.0.0",
    "@shelve/eslint-config": "*",
    "@shelve/types": "*",
    "@shelve/utils": "*",
    "@types/bcryptjs": "^2.4.6",
    "@types/ua-parser-js": "^0.7.39",
    "@vercel/analytics": "^1.2.2",
    "@vue-email/nuxt": "^0.8.19",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "bcryptjs": "^2.4.3",
    "eslint": "^8.57.0",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "latest",
    "nuxt-build-cache": "latest",
    "octokit": "^3.1.2",
    "openai": "^4.29.2",
    "prisma": "^5.11.0",
    "resend": "^3.2.0",
    "ua-parser-js": "^1.0.37",
    "vue": "latest",
    "vue-eslint-parser": "^9.4.2",
    "vue-router": "^4.3.0",
    "vue-sonner": "^1.1.2"
  }
}
