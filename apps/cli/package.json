    "@types/semver": "^7.5.8",
    "automd": "^0.3.6",
    "eslint": "^8.57.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "bin": {
    "shelve": "dist/index.mjs",
    "shelve-cli": "dist/index.mjs"
  },
  "exports": {
    ".": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "citty": "^0.1.6",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "ofetch": "^1.3.3",
    "semver": "^7.6.0",
