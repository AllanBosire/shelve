{
  "name": "@mockline/shelve-cli",
  "version": "0.0.1",
  "description": "A CLI tool for env variables",
  "author": "HugoRCD",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/shelve.git"
  },
  "scripts": {
    "build": "unbuild",
    "start": "node ./dist/index.mjs",
